{
    "name": "idca-preboaring",
    "version": "1.0.0",
    "description": "Description for IDCA_Preboaring",
    "main": "index.js",
    "author": "Walmart",
    "license": "MIT",
    "scripts": {
        "start": "node index.js",
        "start:local": "node target/index.js",
        "build": "npm run build:dev && npm run clean && copy ./dist/* ./target && copy ./package.json ./target && npm run test",
        "clean": "rm -rf ./target && mkdir -p ./target",
        "build:dev": "npm run build-ts && npm run lint",
        "build-ts": "tsc",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
        "watch-node": "nodemon dist/index.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-ts": "npm run lint-fix && tsc -w",
        "copy:config": "copy 'src/config/casbin_conf' 'dist/config/'",
        "test": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx,ts,tsx}"
    },
    "dependencies": {
        "@azure/storage-blob": "^12.3.0",
        "@walmart/esb-signheaders": "^2.0.0",
        "base-64": "^0.1.0",
        "body-parser": "^1.18.3",
        "casbin": "^3.0.7",
        "compression": "^1.7.3",
        "connect-memcached": "^1.0.0",
        "cookie-parser": "^1.4.4",
        "cookie-session": "^1.4.0",
        "cors": "^2.8.5",
        "dotenv": "^8.1.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "helmet": "^3.20.0",
        "lodash": "^4.17.21",
        "memcached": "^2.2.2",
        "moment": "^2.24.0",
        "mongoose": "^5.12.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "node-fetch": "^2.6.0",
        "nodemon": "^1.18.10",
        "passport": "^0.4.0",
        "passport-saml": "^0.33.0",
        "sequelize": "^6.6.2",
        "tedious": "^9.2.1",
        "uuid": "^8.3.2",
        "winston": "^3.3.3",
        "xlsx": "^0.16.9"
    },
    "devDependencies": {
        "@types/compression": "0.0.36",
        "@types/cookie-session": "^2.0.42",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.3",
        "@types/helmet": "0.0.43",
        "@types/jest": "^24.0.18",
        "@types/lodash": "^4.14.168",
        "@types/memcached": "^2.2.6",
        "@types/multer": "^1.3.8",
        "@types/nock": "^10.0.3",
        "@types/node": "^12.7.8",
        "@types/node-fetch": "^2.5.0",
        "@types/passport": "^1.0.6",
        "@types/sequelize": "^4.28.9",
        "@types/xml2js": "^0.4.4",
        "concurrently": "^4.1.0",
        "faker": "^4.1.0",
        "husky": "^6.0.0",
        "jest": "^24.9.0",
        "mssql": "^5.1.0",
        "mz": "^2.7.0",
        "nock": "^10.0.6",
        "performance-now": "^2.1.0",
        "superset": "^1.0.1",
        "ts-jest": "^24.0.2",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3"
    }
}
